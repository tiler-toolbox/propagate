{
  "name": "@tiler/propagate",
  "version": "0.0.0",
  "description": "Utility library to apply time-based propagations on 2D arrays",
  "author": "Florent Dubois <florent.dubois@devaddict.io>",
  "license": "MIT",
  "private": true,
  "keywords": [
    "Tiler",
    "Array",
    "propagation"
  ],
  "scripts": {
    "build": "gulp build",
    "test": "mocha --colors --recursive test",
    "coverage": "istanbul cover node_modules/mocha/bin/_mocha -- --reporter dot --recursive test",
    "lint": "eslint lib test gulpfile.js",
    "preversion": "npm run lint && npm test",
    "version": "sed -r \"/\\[Unreleased\\]\\[unreleased\\]/a \\\n## [$npm_package_version][`date +%F`]\" -i CHANGELOG.md && git add CHANGELOG.md docs/",
    "postversion": "git push --follow-tags"
  },
  "devDependencies": {
    "chai": "^3.4.1",
    "eslint": "^1.10.3",
    "gulp": "^3.9.0",
    "gulp-concat": "^2.6.0",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^1.5.1",
    "istanbul": "^0.4.2",
    "mocha": "^2.3.4"
  }
}
